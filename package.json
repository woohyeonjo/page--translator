{
  "name": "@nnm/page-translator",
  "version": "1.1.0",
  "description": "Page translator using Google Cloud Translation API for Vue.js",
  "private": false,
  "main": "dist/page-translator.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name page-translator src/plugin.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woohyeonjo/page-translator.git"
  },
  "keywords": [
    "translator",
    "translate",
    "translation",
    "google translation",
    "googlecloudtranslation",
    "Vue.js"
  ],
  "author": "Woohyeon Jo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/woohyeonjo/page-translator/issues"
  },
  "homepage": "https://github.com/woohyeonjo/page-translator",
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
